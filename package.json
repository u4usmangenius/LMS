{
  "name": "library_house",
  "version": "1.0.0",
  "description": "A Library Management System application powered by SCS.",
  "main": "main.js",
  "homepage": "./",
  "scripts": {
    "start": "nodemon --watch main.js --exec \"npx electron .\"",
    "prebuild": "rm -rf dist",
    "build:windows": "electron-builder --windows"
  },
  "repository": "https://github.com/u4usmangenius/LMS",
  "keywords": [],
  "author": "Usman Genius <usmantahir.gc.33@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "electron": "^26.6.10",
    "electron-builder": "^24.13.3"
  },
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "build": {
    "appId": "com.Library.com",
    "productName": "Library House",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": {
        "target": "nsis",
        "arch": [
          "x64",
          "ia32"
        ]
      },
      "icon": "icon.png",
      "publisherName": "Usman Genius"
    }
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "electron-store": "^8.1.0",
    "express": "^4.18.2",
    "idb": "^7.1.1",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^3.1.2",
    "mongoose": "^8.3.3",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pdfkit": "^0.14.0",
    "sqlite3": "^5.1.6",
    "sweetalert2-react-content": "^5.0.7"
  }
}
